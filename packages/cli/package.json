{
  "name": "@cra/cli",
  "version": "0.1.0",
  "description": "CRA CLI - Telemetry-first command line interface",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "cra": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@cra/protocol": "*",
    "@cra/trace": "*",
    "@cra/atlas": "*",
    "@cra/runtime": "*",
    "commander": "^11.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "vitest": "^1.1.0"
  },
  "files": [
    "dist"
  ]
}
